#include<stdio.h>
int fact(int n){
    int fact=1;
    for (int i = 1; i <=n; i++)
    {
        fact = fact * i;
    }
    return fact;
}
int main(){
    int n,temp,sum=0,digit;
    printf("Enter n:");
    scanf("%d", &n);
    temp = n;
    while (temp>0)
    {
        digit = temp %10;
        sum+=fact(digit);
        temp/=10;
    }
    if(sum==n){
        printf("This is a strong number: %d", n);
    }
    else{
        printf("This is a weak number : %d", n);
    }
    return 0;
}
